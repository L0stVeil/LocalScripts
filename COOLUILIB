            tind.Size = UDim2.new(0.5, 0, 1, 0)
            
            local st = def
            
            tf.InputBegan:Connect(function(inp)
                if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                    st = not st
                    tind.BackgroundColor3 = st and cg_red or cg_blk
                    tind.Position = st and UDim2.new(0.5, 0, 0, 0) or UDim2.new(0, 0, 0, 0)
                    cb(st)
                end
            end)
            
            return pg
        end
        
