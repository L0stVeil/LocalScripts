        lc.BackgroundColor3 = cg_blk
        lc.BorderColor3 = cg_red
        lc.BorderSizePixel = 0
        lc.Position = UDim2.new(0, 0, 0, 30)
        lc.Size = UDim2.new(1, 0, 1, -30)
        lc.ScrollBarThickness = 4
        lc.CanvasSize = UDim2.new(0, 0, 0, 0)
        lc.ScrollingDirection = Enum.ScrollingDirection.Y
        lc.AutomaticCanvasSize = Enum.AutomaticSize.Y
        
        local ll = Instance.new("UIListLayout")
        ll.Parent = lc
        ll.SortOrder = Enum.SortOrder.LayoutOrder
        ll.Padding = UDim.new(0, 5)
        
        local rs = Instance.new("Frame")
        rs.Name = "RightSide"
        rs.Parent = pf
        rs.BackgroundColor3 = cg_blk
        rs.BorderColor3 = cg_red
        rs.BorderSizePixel = 2
        rs.Position = UDim2.new(0.5, 3, 0, 0)
        rs.Size = UDim2.new(0.5, -3, 1, 0)
        
        local rt = Instance.new("TextLabel")
        rt.Name = "RightTitle"
        rt.Parent = rs
        rt.BackgroundColor3 = cg_blk
        rt.BorderColor3 = cg_red
        rt.BorderSizePixel = 2
        rt.Position = UDim2.new(0, 0, 0, 0)
        rt.Size = UDim2.new(1, 0, 0, 30)
        rt.Font = cg_font
        rt.Text = "FE Guis"
        rt.TextColor3 = cg_wht
        rt.TextSize = 16
        
        local rc = Instance.new("ScrollingFrame")
        rc.Name = "RightContent"
        rc.Parent = rs
        rc.BackgroundColor3 = cg_blk
        rc.BorderColor3 = cg_red
        rc.BorderSizePixel = 0
        rc.Position = UDim2.new(0, 0, 0, 30)
        rc.Size = UDim2.new(1, 0, 1, -30)
        rc.ScrollBarThickness = 4
        rc.CanvasSize = UDim2.new(0, 0, 0, 0)
        rc.ScrollingDirection = Enum.ScrollingDirection.Y
        rc.AutomaticCanvasSize = Enum.AutomaticSize.Y
        
