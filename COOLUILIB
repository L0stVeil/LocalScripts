        pg.AddLeftTextBox = function(txt, def, cb)
            local tbf = Instance.new("Frame")
            tbf.Name = "TBox_" .. txt
            tbf.Parent = lc
            tbf.BackgroundColor3 = cg_blk
            tbf.BorderColor3 = cg_red
            tbf.BorderSizePixel = 2
            tbf.Size = UDim2.new(1, -10, 0, 30)
            tbf.Position = UDim2.new(0, 5, 0, 0)
            
            local tbl = Instance.new("TextLabel")
            tbl.Name = "Label"
            tbl.Parent = tbf
            tbl.BackgroundTransparency = 1
            tbl.Size = UDim2.new(0.3, 0, 1, 0)
            tbl.Font = cg_font
            tbl.Text = txt
            tbl.TextColor3 = cg_wht
            tbl.TextSize = 14
            tbl.TextXAlignment = Enum.TextXAlignment.Left
            tbl.Position = UDim2.new(0, 10, 0, 0)
            
            local tbi = Instance.new("TextBox")
            tbi.Name = "Input"
            tbi.Parent = tbf
            tbi.BackgroundColor3 = cg_blk
            tbi.BorderColor3 = cg_red
            tbi.BorderSizePixel = 1
            tbi.Position = UDim2.new(0.35, 0, 0.1, 0)
            tbi.Size = UDim2.new(0.6, 0, 0.8, 0)
            tbi.Font = cg_font
            tbi.Text = def or ""
            tbi.PlaceholderText = "Enter text..."
            tbi.TextColor3 = cg_wht
            tbi.TextSize = 14
            
            tbi.FocusLost:Connect(function(enter)
                if enter then
                    cb(tbi.Text)
                end
            end)
            
            return pg
        end
        
        pg.AddRightTextBox = function(txt, def, cb)
            local tbf = Instance.new("Frame")
            tbf.Name = "TBox_" .. txt
