        pg.AddLeftDropdown = function(txt, opts, cb)
            local ddf = Instance.new("Frame")
            ddf.Name = "Drop_" .. txt
            ddf.Parent = lc
            ddf.BackgroundColor3 = cg_blk
            ddf.BorderColor3 = cg_red
            ddf.BorderSizePixel = 2
            ddf.Size = UDim2.new(1, -10, 0, 30)
            ddf.Position = UDim2.new(0, 5, 0, 0)
            
            local ddb = Instance.new("TextButton")
            ddb.Name = "DropBtn"
            ddb.Parent = ddf
            ddb.BackgroundTransparency = 1
            ddb.Size = UDim2.new(1, 0, 1, 0)
            ddb.Font = cg_font
            ddb.Text = txt .. ": " .. opts[1]
            ddb.TextColor3 = cg_wht
            ddb.TextSize = 14
            
            local ddl = Instance.new("Frame")
            ddl.Name = "DropList"
            ddl.Parent = ddf
            ddl.BackgroundColor3 = cg_blk
            ddl.BorderColor3 = cg_red
            ddl.BorderSizePixel = 2
            ddl.Position = UDim2.new(0, 0, 1, 0)
            ddl.Size = UDim2.new(1, 0, 0, #opts * 30)
            ddl.Visible = false
            ddl.ZIndex = 10
            
            local ddll = Instance.new("UIListLayout")
            ddll.Parent = ddl
            ddll.SortOrder = Enum.SortOrder.LayoutOrder
            
            local sel = opts[1]
            
            for i, opt in ipairs(opts) do
                local ddopt = Instance.new("TextButton")
                ddopt.Name = "Opt_" .. opt
                ddopt.Parent = ddl
                ddopt.BackgroundColor3 = cg_blk
                ddopt.BorderColor3 = cg_red
                ddopt.BorderSizePixel = 1
                ddopt.Size = UDim2.new(1, 0, 0, 30)
                ddopt.Font = cg_font
                ddopt.Text = opt
                ddopt.TextColor3 = cg_wht
                ddopt.TextSize = 14
                ddopt.ZIndex = 10
                
                ddopt.MouseButton1Click:Connect(function()
                    sel = opt
                    ddb.Text = txt .. ": " .. sel
                    ddl.Visible = false
                    cb(sel)
                end)
            end
            
            ddb.MouseButton1Click:Connect(function()
                ddl.Visible = not ddl.Visible
            end)
            
            return pg
        end
        
        pg.AddRightDropdown = function(txt, opts, cb)
