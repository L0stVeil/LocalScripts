            btn.TextColor3 = cg_wht
            btn.TextSize = 14
            
            btn.MouseButton1Click:Connect(function()
                cb()
            end)
            
            return pg
        end
        
        pg.AddLeftToggle = function(txt, def, cb)
            local tf = Instance.new("Frame")
            tf.Name = "Tgl_" .. txt
            tf.Parent = lc
            tf.BackgroundColor3 = cg_blk
            tf.BorderColor3 = cg_red
            tf.BorderSizePixel = 2
            tf.Size = UDim2.new(1, -10, 0, 30)
            tf.Position = UDim2.new(0, 5, 0, 0)
            
            local tl = Instance.new("TextLabel")
            tl.Name = "Label"
            tl.Parent = tf
            tl.BackgroundTransparency = 1
            tl.Size = UDim2.new(0.7, 0, 1, 0)
            tl.Font = cg_font
            tl.Text = txt
            tl.TextColor3 = cg_wht
            tl.TextSize = 14
            tl.TextXAlignment = Enum.TextXAlignment.Left
            tl.Position = UDim2.new(0, 10, 0, 0)
            
            local tbg = Instance.new("Frame")
            tbg.Name = "TglFrame"
            tbg.Parent = tf
            tbg.BackgroundColor3 = cg_blk
            tbg.BorderColor3 = cg_red
            tbg.BorderSizePixel = 2
            tbg.Position = UDim2.new(0.8, 0, 0.5, -10)
            tbg.Size = UDim2.new(0, 40, 0, 20)
            
            local tind = Instance.new("Frame")
            tind.Name = "Indicator"
            tind.Parent = tbg
            tind.BackgroundColor3 = def and cg_red or cg_blk
            tind.BorderColor3 = cg_red
            tind.BorderSizePixel = 1
            tind.Position = def and UDim2.new(0.5, 0, 0, 0) or UDim2.new(0, 0, 0, 0)
            tind.Size = UDim2.new(0.5, 0, 1, 0)
            
            local st = def
            
            tf.InputBegan:Connect(function(inp)
                if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                    st = not st
                    tind.BackgroundColor3 = st and cg_red or cg_blk
                    tind.Position = st and UDim2.new(0.5, 0, 0, 0) or UDim2.new(0, 0, 0, 0)
                    cb(st)
                end
            end)
            
            return pg
        end
        
