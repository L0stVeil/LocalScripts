local cg = {}
local cg_ui = game:GetService("CoreGui")
local cg_uis = game:GetService("UserInputService")
local cg_ts = game:GetService("TweenService")
local cg_hs = game:GetService("HttpService")
local cg_rs = game:GetService("RunService")
local cg_plrs = game:GetService("Players")
local cg_lp = cg_plrs.LocalPlayer
local cg_m = cg_lp:GetMouse()
local cg_gui = cg_ui:FindFirstChild("c00lk1dd"..math.random(1000,9999))
local cg_blk = Color3.new(0,0,0)
local cg_red = Color3.new(255/255,0/255,0/255)
local cg_wht = Color3.new(255/255,255/255,255/255)
local cg_font = "SourceSans"

if cg_gui then
    cg_gui:Destroy()
end

local function md(frm, prnt)
    local dg = false
    local mp
    local sp
    local op
    local mv
    
    frm.InputBegan:Connect(function(inp)
        if inp.UserInputType == Enum.UserInputType.MouseButton1 or inp.UserInputType == Enum.UserInputType.Touch then
            dg = true
            sp = inp.Position
            op = frm.Position
            
            inp.Changed:Connect(function()
                if inp.UserInputState == Enum.UserInputState.End then
                    dg = false
                end
            end)
        end
    end)
    
    frm.InputChanged:Connect(function(inp)
        if inp.UserInputType == Enum.UserInputType.MouseMovement or inp.UserInputType == Enum.UserInputType.Touch then
            mp = inp
        end
    end)
    
    cg_uis.InputChanged:Connect(function(inp)
        if inp == mp and dg then
            mv = inp.Position - sp
            frm.Position = UDim2.new(op.X.Scale, op.X.Offset + mv.X, op.Y.Scale, op.Y.Offset + mv.Y)
        end
    end)
end

local sg = Instance.new("ScreenGui")
sg.Name = "c00lk1dd"..math.random(1000,9999)
sg.Parent = cg_ui
sg.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
sg.ResetOnSpawn = false

cg.CreateWindow = function(ttl)
    local win = {}
    
    local mf = Instance.new("Frame")
    mf.Name = "MainFrame"
    mf.Parent = sg
    mf.BackgroundColor3 = cg_blk
    mf.BorderColor3 = cg_red
    mf.BorderSizePixel = 2
    mf.Position = UDim2.new(0.5, -150, 0.5, -200)
    mf.Size = UDim2.new(0, 300, 0, 400)
    mf.Active = true
    
    md(mf)
    
    local tb = Instance.new("Frame")
    tb.Name = "TopBar"
    tb.Parent = mf
    tb.BackgroundColor3 = cg_blk
    tb.BorderColor3 = cg_red
    tb.BorderSizePixel = 2
    tb.Size = UDim2.new(1, 0, 0, 40)
    
    local tl = Instance.new("TextLabel")
    tl.Name = "Title"
    tl.Parent = tb
    tl.BackgroundTransparency = 1
    tl.Size = UDim2.new(1, 0, 1, 0)
    tl.Font = cg_font
    tl.Text = ttl or "c00lk1dd"
    tl.TextColor3 = cg_wht
    tl.TextSize = 18
    
