    cb.Position = UDim2.new(1, -40, 0, 0)
    cb.Size = UDim2.new(0, 40, 1, 0)
    cb.Font = cg_font
    cb.Text = "X"
    cb.TextColor3 = cg_wht
    cb.TextSize = 18
    
    cb.MouseButton1Click:Connect(function()
        mf.Visible = not mf.Visible
    end)
    
    local pn = Instance.new("Frame")
    pn.Name = "PageNav"
    pn.Parent = mf
    pn.BackgroundColor3 = cg_blk
    pn.BorderColor3 = cg_red
    pn.BorderSizePixel = 2
    pn.Position = UDim2.new(0, 0, 0, 40)
    pn.Size = UDim2.new(1, 0, 0, 40)
    
    local lb = Instance.new("TextButton")
    lb.Name = "LeftBtn"
    lb.Parent = pn
    lb.BackgroundColor3 = cg_blk
    lb.BorderColor3 = cg_red
    lb.BorderSizePixel = 2
    lb.Position = UDim2.new(0, 0, 0, 0)
    lb.Size = UDim2.new(0.5, -3, 1, 0)
    lb.Font = cg_font
    lb.Text = "<"
    lb.TextColor3 = cg_wht
    lb.TextSize = 24
    
    local rb = Instance.new("TextButton")
    rb.Name = "RightBtn"
    rb.Parent = pn
    rb.BackgroundColor3 = cg_blk
    rb.BorderColor3 = cg_red
    rb.BorderSizePixel = 2
    rb.Position = UDim2.new(0.5, 3, 0, 0)
    rb.Size = UDim2.new(0.5, -3, 1, 0)
    rb.Font = cg_font
    rb.Text = ">"
    rb.TextColor3 = cg_wht
    rb.TextSize = 24
    
    local pg = Instance.new("Frame")
    pg.Name = "Pages"
    pg.Parent = mf
    pg.BackgroundColor3 = cg_blk
    pg.BorderColor3 = cg_red
    pg.BorderSizePixel = 2
    pg.Position = UDim2.new(0, 0, 0, 80)
    pg.Size = UDim2.new(1, 0, 1, -80)
    
    local pgs = {}
    local cp = 1
    local pc = 0
    
    local function upd()
        for i, p in pairs(pgs) do
            p.Visible = i == cp
        end
    end
    
    lb.MouseButton1Click:Connect(function()
        if cp > 1 then
            cp = cp - 1
            upd()
        end
    end)
    
    rb.MouseButton1Click:Connect(function()
        if cp < #pgs then
            cp = cp + 1
            upd()
        end
    end)
    
    win.CreatePage = function()
        pc = pc + 1
        local pg = {}
        
        local pf = Instance.new("Frame")
        pf.Name = "Page_" .. pc
        pf.Parent = pg
        pf.BackgroundColor3 = cg_blk
        pf.BorderColor3 = cg_red
        pf.BorderSizePixel = 2
        pf.Size = UDim2.new(1, 0, 1, 0)
        pf.Visible = #pgs == 0
        
        local ls = Instance.new("Frame")
        ls.Name = "LeftSide"
        ls.Parent = pf
        ls.BackgroundColor3 = cg_blk
        ls.BorderColor3 = cg_red
        ls.BorderSizePixel = 2
        ls.Position = UDim2.new(0, 0, 0, 0)
        ls.Size = UDim2.new(0.5, -3, 1, 0)
        
        local lt = Instance.new("TextLabel")
        lt.Name = "LeftTitle"
        lt.Parent = ls
        lt.BackgroundColor3 = cg_blk
        lt.BorderColor3 = cg_red
        lt.BorderSizePixel = 2
        lt.Position = UDim2.new(0, 0, 0, 0)
        lt.Size = UDim2.new(1, 0, 0, 30)
