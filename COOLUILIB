            sll.Name = "Label"
            sll.Parent = slf
            sll.BackgroundTransparency = 1
            sll.Size = UDim2.new(1, 0, 0, 20)
            sll.Font = cg_font
            sll.Text = txt .. ": " .. def
            sll.TextColor3 = cg_wht
            sll.TextSize = 14
            
            local slb = Instance.new("Frame")
            slb.Name = "SliderBack"
            slb.Parent = slf
            slb.BackgroundColor3 = cg_blk
            slb.BorderColor3 = cg_red
            slb.BorderSizePixel = 2
            slb.Position = UDim2.new(0, 10, 0, 30)
            slb.Size = UDim2.new(1, -20, 0, 10)
            
            local slf = Instance.new("Frame")
            slf.Name = "SliderFill"
            slf.Parent = slb
            slf.BackgroundColor3 = cg_red
            slf.BorderSizePixel = 0
            slf.Size = UDim2.new((def - min) / (max - min), 0, 1, 0)
            
            local slbt = Instance.new("TextButton")
            slbt.Name = "SliderBtn"
            slbt.Parent = slb
            slbt.BackgroundTransparency = 1
            slbt.Size = UDim2.new(1, 0, 1, 0)
            slbt.Text = ""
            
            local val = def
            
            local function upd(pos)
                local rat = math.clamp((pos - slb.AbsolutePosition.X) / slb.AbsoluteSize.X, 0, 1)
                local nval = min + (max - min) * rat
                nval = math.floor(nval)
                val = nval
                slf.Size = UDim2.new(rat, 0, 1, 0)
                sll.Text = txt .. ": " .. val
                cb(val)
            end
            
            slbt.InputBegan:Connect(function(inp)
                if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                    upd(inp.Position.X)
                    local dg = true
                    local cn
                    
                    cn = cg_uis.InputChanged:Connect(function(inp)
                        if inp.UserInputType == Enum.UserInputType.MouseMovement then
                            upd(inp.Position.X)
                        end
                    end)
                    
                    cg_uis.InputEnded:Connect(function(inp)
                        if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                            dg = false
                            cn:Disconnect()
                        end
                    end)
                end
            end)
            
            return pg
        end
        
        pg.AddRightSlider = function(txt, min, max, def, cb)
            local slf = Instance.new("Frame")
            slf.Name = "Slider_" .. txt
            slf.Parent = rc
            slf.BackgroundColor3 = cg_blk
            slf.BorderColor3 = cg_red
            slf.BorderSizePixel = 2
            slf.Size = UDim2.new(1, -10, 0, 50)
            slf.Position = UDim2.new(0, 5, 0, 0)
            
            local sll = Instance.new("TextLabel")
            sll.Name = "Label"
            sll.Parent = slf
            sll.BackgroundTransparency = 1
            sll.Size = UDim2.new(1, 0, 0, 20)
            sll.Font = cg_font
            sll.Text = txt .. ": " .. def
            sll.TextColor3 = cg_wht
            sll.TextSize = 14
            
            local slb = Instance.new("Frame")
            slb.Name = "SliderBack"
            slb.Parent = slf
            slb.BackgroundColor3 = cg_blk
            slb.BorderColor3 = cg_red
            slb.BorderSizePixel = 2
            slb.Position = UDim2.new(0, 10, 0, 30)
            slb.Size = UDim2.new(1, -20, 0, 10)
            
            local slf = Instance.new("Frame")
            slf.Name = "SliderFill"
            slf.Parent = slb
            slf.BackgroundColor3 = cg_red
            slf.BorderSizePixel = 0
            slf.Size = UDim2.new((def - min) / (max - min), 0, 1, 0)
            
            local slbt = Instance.new("TextButton")
            slbt.Name = "SliderBtn"
            slbt.Parent = slb
            slbt.BackgroundTransparency = 1
            slbt.Size = UDim2.new(1, 0, 1, 0)
            slbt.Text = ""
            
            local val = def
            
            local function upd(pos)
                local rat = math.clamp((pos - slb.AbsolutePosition.X) / slb.AbsoluteSize.X, 0, 1)
                local nval = min + (max - min) * rat
                nval = math.floor(nval)
                val = nval
                slf.Size = UDim2.new(rat, 0, 1, 0)
                sll.Text = txt .. ": " .. val
                cb(val)
            end
            
            slbt.InputBegan:Connect(function(inp)
                if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                    upd(inp.Position.X)
                    local dg = true
                    local cn
                    
                    cn = cg_uis.InputChanged:Connect(function(inp)
                        if inp.UserInputType == Enum.UserInputType.MouseMovement then
                            upd(inp.Position.X)
                        end
                    end)
                    
                    cg_uis.InputEnded:Connect(function(inp)
                        if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                            dg = false
                            cn:Disconnect()
                        end
                    end)
                end
            end)
            
            return pg
        end
        
        pg.AddColorPicker = function(txt, def, cb)
            local cpf = Instance.new("Frame")
            cpf.Name = "ColorPicker_" .. txt
            cpf.Parent = lc
            cpf.BackgroundColor3 = cg_blk
            cpf.BorderColor3 = cg_red
            cpf.BorderSizePixel = 2
            cpf.Size = UDim2.new(1, -10, 0, 30)
            cpf.Position = UDim2.new(0, 5, 0, 0)
            
            local cpl = Instance.new("TextLabel")
            cpl.Name = "Label"
            cpl.Parent = cpf
            cpl.BackgroundTransparency = 1
            cpl.Size = UDim2.new(0.7, 0, 1, 0)
            cpl.Font = cg_font
            cpl.Text = txt
            cpl.TextColor3 = cg_wht
            cpl.TextSize = 14
            cpl.TextXAlignment = Enum.TextXAlignment.Left
            cpl.Position = UDim2.new(0, 10, 0, 0)
            
            local cpb = Instance.new("TextButton")
            cpb.Name = "ColorBtn"
            cpb.Parent = cpf
            cpb.BackgroundColor3 = def or Color3.fromRGB(255, 0, 0)
            cpb.BorderColor3 = cg_red
            cpb.BorderSizePixel = 2
            cpb.Position = UDim2.new(0.8, 0, 0.5, -10)
            cpb.Size = UDim2.new(0, 40, 0, 20)
            cpb.Text = ""
            
            local cpc = def or Color3.fromRGB(255, 0, 0)
            
            cpb.MouseButton1Click:Connect(function()
                local cp = Instance.new("Frame")
                cp.Name = "ColorPickerPanel"
                cp.Parent = sg
                cp.BackgroundColor3 = cg_blk
                cp.BorderColor3 = cg_red
                cp.BorderSizePixel = 2
                cp.Position = UDim2.new(0.5, -100, 0.5, -100)
                cp.Size = UDim2.new(0, 200, 0, 200)
                cp.ZIndex = 100
                
                local r = Instance.new("TextBox")
                r.Name = "R"
                r.Parent = cp
                r.BackgroundColor3 = cg_blk
                r.BorderColor3 = cg_red
                r.BorderSizePixel = 2
                r.Position = UDim2.new(0, 10, 0, 10)
                r.Size = UDim2.new(0, 50, 0, 30)
                r.Font = cg_font
                r.Text = tostring(math.floor(cpc.R * 255))
                r.TextColor3 = cg_wht
                r.TextSize = 14
                r.ZIndex = 101
                
                local rl = Instance.new("TextLabel")
                rl.Name = "RLabel"
                rl.Parent = cp
                rl.BackgroundTransparency = 1
                rl.Position = UDim2.new(0, 70, 0, 10)
                rl.Size = UDim2.new(0, 20, 0, 30)
                rl.Font = cg_font
                rl.Text = "R"
                rl.TextColor3 = cg_wht
                rl.TextSize = 14
                rl.ZIndex = 101
                
                local g = Instance.new("TextBox")
                g.Name = "G"
                g.Parent = cp
                g.BackgroundColor3 = cg_blk
                g.BorderColor3 = cg_red
                g.BorderSizePixel = 2
                g.Position = UDim2.new(0, 10, 0, 50)
                g.Size = UDim2.new(0, 50, 0, 30)
                g.Font = cg_font
                g.Text = tostring(math.floor(cpc.G * 255))
                g.TextColor3 = cg_wht
                g.TextSize = 14
                g.ZIndex = 101
                
                local gl = Instance.new("TextLabel")
                gl.Name = "GLabel"
                gl.Parent = cp
                gl.BackgroundTransparency = 1
                gl.Position = UDim2.new(0, 70, 0, 50)
                gl.Size = UDim2.new(0, 20, 0, 30)
                gl.Font = cg_font
                gl.Text = "G"
                gl.TextColor3 = cg_wht
                gl.TextSize = 14
                gl.ZIndex = 101
                
                local b = Instance.new("TextBox")
                b.Name = "B"
                b.Parent = cp
                b.BackgroundColor3 = cg_blk
                b.BorderColor3 = cg_red
                b.BorderSizePixel = 2
                b.Position = UDim2.new(0, 10, 0, 90)
                b.Size = UDim2.new(0, 50, 0, 30)
                b.Font = cg_font
                b.Text = tostring(math.floor(cpc.B * 255))
                b.TextColor3 = cg_wht
                b.TextSize = 14
                b.ZIndex = 101
                
                local bl = Instance.new("TextLabel")
                bl.Name = "BLabel"
                bl.Parent = cp
                bl.BackgroundTransparency = 1
                bl.Position = UDim2.new(0, 70, 0, 90)
                bl.Size = UDim2.new(0, 20, 0, 30)
                bl.Font = cg_font
                bl.Text = "B"
                bl.TextColor3 = cg_wht
                bl.TextSize = 14
                bl.ZIndex = 101
                
                local preview = Instance.new("Frame")
                preview.Name = "Preview"
                preview.Parent = cp
                preview.BackgroundColor3 = cpc
                preview.BorderColor3 = cg_red
                preview.BorderSizePixel = 2
                preview.Position = UDim2.new(0, 100, 0, 10)
                preview.Size = UDim2.new(0, 90, 0, 110)
                preview.ZIndex = 101
                
                local apply = Instance.new("TextButton")
                apply.Name = "Apply"
                apply.Parent = cp
                apply.BackgroundColor3 = cg_blk
                apply.BorderColor3 = cg_red
                apply.BorderSizePixel = 2
                apply.Position = UDim2.new(0, 10, 0, 150)
                apply.Size = UDim2.new(0, 80, 0, 30)
                apply.Font = cg_font
                apply.Text = "Apply"
                apply.TextColor3 = cg_wht
                apply.TextSize = 14
                apply.ZIndex = 101
                
                local cancel = Instance.new("TextButton")
                cancel.Name = "Cancel"
                cancel.Parent = cp
                cancel.BackgroundColor3 = cg_blk
                cancel.BorderColor3 = cg_red
                cancel.BorderSizePixel = 2
                cancel.Position = UDim2.new(0, 110, 0, 150)
                cancel.Size = UDim2.new(0, 80, 0, 30)
                cancel.Font = cg_font
                cancel.Text = "Cancel"
                cancel.TextColor3 = cg_wht
                cancel.TextSize = 14
                cancel.ZIndex = 101
                
                local function updatePreview()
                    local rv = tonumber(r.Text) or 0
                    local gv = tonumber(g.Text) or 0
                    local bv = tonumber(b.Text) or 0
                    
                    rv = math.clamp(rv, 0, 255) / 255
                    gv = math.clamp(gv, 0, 255) / 255
                    bv = math.clamp(bv, 0, 255) / 255
                    
                    preview.BackgroundColor3 = Color3.new(rv, gv, bv)
                end
                
                r.FocusLost:Connect(updatePreview)
                g.FocusLost:Connect(updatePreview)
                b.FocusLost:Connect(updatePreview)
                
                apply.MouseButton1Click:Connect(function()
                    local rv = tonumber(r.Text) or 0
                    local gv = tonumber(g.Text) or 0
                    local bv = tonumber(b.Text) or 0
                    
                    rv = math.clamp(rv, 0, 255) / 255
                    gv = math.clamp(gv, 0, 255) / 255
                    bv = math.clamp(bv, 0, 255) / 255
                    
                    cpc = Color3.new(rv, gv, bv)
                    cpb.BackgroundColor3 = cpc
                    cb(cpc)
                    cp:Destroy()
                end)
                
                cancel.MouseButton1Click:Connect(function()
                    cp:Destroy()
                end)
                
                md(cp)
            end)
            
            return pg
        end
        
        return pg
    end
    
    win.Notify = function(ttl, txt, dur)
        local ntf = Instance.new("Frame")
        ntf.Name = "Notification"
        ntf.Parent = sg
        ntf.BackgroundColor3 = cg_blk
        ntf.BorderColor3 = cg_red
        ntf.BorderSizePixel = 2
        ntf.Position = UDim2.new(1, -300, 1, -100)
        ntf.Size = UDim2.new(0, 250, 0, 80)
        ntf.ZIndex = 100
        
        local ntft = Instance.new("TextLabel")
        ntft.Name = "Title"
        ntft.Parent = ntf
        ntft.BackgroundTransparency = 1
        ntft.Size = UDim2.new(1, 0, 0, 30)
        ntft.Font = cg_font
        ntft.Text = ttl
        ntft.TextColor3 = cg_wht
        ntft.TextSize = 18
        ntft.ZIndex = 101
        
        local ntfm = Instance.new("TextLabel")
        ntfm.Name = "Message"
        ntfm.Parent = ntf
        ntfm.BackgroundTransparency = 1
        ntfm.Position = UDim2.new(0, 0, 0, 30)
        ntfm.Size = UDim2.new(1, 0, 1, -30)
        ntfm.Font = cg_font
        ntfm.Text = txt
        ntfm.TextColor3 = cg_wht
        ntfm.TextSize = 14
        ntfm.TextWrapped = true
        ntfm.ZIndex = 101
        
        cg_ts:Create(ntf, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(1, -270, 1, -100)}):Play()
        
        task.delay(dur or 5, function()
            cg_ts:Create(ntf, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(1, 0, 1, -100)}):Play()
            task.delay(0.5, function()
                ntf:Destroy()
            end)
        end)
    end
    
    local tgb = Instance.new("TextButton")
    tgb.Name = "ToggleBtn"
    tgb.Parent = sg
    tgb.BackgroundColor3 = cg_blk
    tgb.BorderColor3 = cg_red
    tgb.BorderSizePixel = 2
    tgb.Position = UDim2.new(0, 10, 0, 10)
    tgb.Size = UDim2.new(0, 100, 0, 30)
    tgb.Font = cg_font
    tgb.Text = "Toggle UI"
    tgb.TextColor3 = cg_wht
    tgb.TextSize = 14
    
    tgb.MouseButton1Click:Connect(function()
        mf.Visible = not mf.Visible
    end)
    
    return win
end

return cg
