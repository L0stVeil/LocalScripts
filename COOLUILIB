        rl.Parent = rc
        rl.SortOrder = Enum.SortOrder.LayoutOrder
        rl.Padding = UDim.new(0, 5)
        
        table.insert(pgs, pf)
        
        pg.SetLeftTitle = function(txt)
            lt.Text = txt
            return pg
        end
        
        pg.SetRightTitle = function(txt)
            rt.Text = txt
            return pg
        end
        
        pg.AddLeftButton = function(txt, cb)
            local btn = Instance.new("TextButton")
            btn.Name = "Btn_" .. txt
            btn.Parent = lc
            btn.BackgroundColor3 = cg_blk
            btn.BorderColor3 = cg_red
            btn.BorderSizePixel = 2
            btn.Size = UDim2.new(1, -10, 0, 30)
            btn.Position = UDim2.new(0, 5, 0, 0)
            btn.Font = cg_font
            btn.Text = txt
            btn.TextColor3 = cg_wht
            btn.TextSize = 14
            
            btn.MouseButton1Click:Connect(function()
                cb()
            end)
            
            return pg
        end
        
        pg.AddRightButton = function(txt, cb)
            local btn = Instance.new("TextButton")
            btn.Name = "Btn_" .. txt
            btn.Parent = rc
            btn.BackgroundColor3 = cg_blk
